chan p7, p4, p3, p9, p2, p1, p5, p6, p8, s2, s4, s3, s1;
const int r1 = 2, r2 = 3, r3 = 1, r4 = 1, r5 = 1, r6 = 1;
const int ds1 = 1, ds2 = 1, ds3 = 1, ds4 = 3;
const int ns1 = 5, ns2 = 3, ns3 = 4, ns4 = 6;
int As2[6], As4[6], As3[6], As1[6];
int cs2 = 0, os2 = 0, cs4 = 0, os4 = 0, cs3 = 0, os3 = 0, cs1 = 0, os1 = 0;

void enqueue(int &amp;A[6], int n, int &amp;cs, int &amp;os, int p) {
    if (cs == n){
        int i;        
        for (i=0; i&lt;=n-2; i++){
            A[i] = A[i+1];
        }
        A[n-1] = p;
        os = 1-os;
    }
    else {
        A[cs] = p;
        cs++;
    }
}

void dequeue(int &amp;A[6], int n, int &amp;cs) {    
    int i;
    if (cs == 1){
        A[0] = 0;
    }
    if (cs &gt; 1) {
        for (i=0; i&lt;=cs-2; i++){
            A[i] = A[i+1];
        }
        A[cs-1] = 0;
    }
    cs--;
    if (cs &lt; 0){
        cs = 0;
    }
}
