<nta>
	<declaration>
chan p3, p1, p2, s2, s1;
const int t1 = 2, t2 = 3;
const int ds1 = 1, ds2 = 10;
const int ns1 = 5, ns2 = 3;
int As2[100], As1[100];
int cs2 = 0, os2 = 0, cs1 = 0, os1 = 0;

void enqueue(int &amp;A[100], int n, int &amp;cs, int &amp;os, int p) {
    if (cs == n){
        int i;        
        for (i=0; i&lt;=n-2; i++){
            A[i] = A[i+1];
        }
        A[n-1] = p;
        os = 1-os;
    }
    else {
        A[cs] = p;
        cs++;
    }
}

void dequeue(int &amp;A[100], int n, int &amp;cs) {    
    int i;
    if (cs == 1){
        A[0] = 0;
    }
    if (cs &gt; 1) {
        for (i=0; i&lt;=cs-2; i++){
            A[i] = A[i+1];
        }
        A[cs-1] = 0;
    }
    cs--;
    if (cs &lt; 0){
        cs = 0;
    }
}
</declaration>
